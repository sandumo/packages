    "@prisma/client": "^5.12.1",
    "api-client": "workspace:*",
    "aws-sdk": "^2.1309.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
