{
  "extends": "tsconfig/ts-library-transpiled.json",
  "include": ["."],
  "exclude": ["dist", "build", "node_modules"],
  "compilerOptions": {
    "outDir": "./dist",
    "experimentalDecorators": true,
    "isolatedModules": false,
    "target": "ES6",
    "strictPropertyInitialization": false,
    "strictNullChecks": false
  }
}
